@model WizLoad.ApiClient.BasketModel
@{
    ViewData["Title"] = "Basket";
}

<div class="container">
    <div class="container-box">
        <h2>Basket</h2>
        @if(Model.ProductIds?.Count > 0)
        {
            @foreach(var product in Model.ProductIds)
            {
                <h4>@product.ToString()</h4>
            }
        }
    </div>
</div>